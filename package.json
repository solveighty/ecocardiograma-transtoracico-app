{
  "name": "ecocardiograma-transtoracico-app",
  "private": true,
  "version": "0.0.1",
  "description": "Aplicación de ecocardiograma transtorácico para gestión de exámenes cardiológicos",
  "author": "Sistema de Ecocardiograma",
  "scripts": {
    "dev": "vite",
    "start": "vite --config vite.config.ts",
    "electron": "wait-on tcp:5173 && electron .",
    "electron:dev": "concurrently -k \"npm run start\" \"npm run electron\"",
    "build": "vite build && electron-builder --dir",
    "build:debug": "vite build && electron-builder --dir",
    "build:release": "vite build && electron-builder",
    "test:prod": "vite build && cross-env NODE_ENV=production electron .",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "docxtemplater": "^3.65.3",
    "lucide-react": "^0.536.0",
    "pizzip": "^3.2.0",
    "react": "^18.2.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.7.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react": "^4.0.4",
    "asar": "^3.2.0",
    "autoprefixer": "^10.4.15",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.48.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-electron": "^0.14.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "wait-on": "^8.0.4"
  },
  "main": "dist-electron/main.js"
}
